{
    "name": "@hualala/platform-sale",
    "version": "0.3.2",
    "description": "商户平台会员业务包，基于@hualala/platform-base定义的规范实现。",
    "main": "index.js",
    "scripts": {
        "debug:server": "cd ../platform && npm run debug:server",
        "debug:client": "cd ../platform && npm link ../platform-sale && cross-env PACKAGE_NAME=sale npm run debug:client",
        "prepublishOnly": "rimraf lib && cross-env BABEL_ENV=production babel -D src --out-dir lib",
        "precommit": "lint-staged",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "lint-staged": {
        "src/**/*.{js,jsx}": [
            "eslint --fix",
            "git add"
        ]
    },
    "author": "C. <chenhuangfang@hualala.com>",
    "dependencies": {
        "@hualala/platform-base": "^1.8.5",
        "@hualala/platform-components": "~1.4.10",
        "@hualala/react-lazyload": "^2.3.0",
        "antd": "2.8.0",
        "jquery": "^3.2.1",
        "prop-types": "^15.6.0",
        "rc-color-picker": "^1.2.3",
        "react": "15.4.2",
        "react-dom": "15.4.2",
        "react-redux": "5.0.2",
        "rxjs": "^5.5.0"
    },
    "devDependencies": {
        "babel-cli": "6.24.1",
        "babel-core": "^6.26.0",
        "babel-eslint": "7.2.3",
        "babel-loader": "7.0.0",
        "babel-plugin-css-modules-transform": "1.2.7",
        "babel-plugin-import": "1.1.1",
        "babel-plugin-lodash": "3.2.11",
        "babel-plugin-transform-decorators-legacy": "1.3.4",
        "babel-plugin-transform-es2015-spread": "6.22.0",
        "babel-plugin-transform-runtime": "6.23.0",
        "babel-preset-env": "1.4.0",
        "babel-preset-es2015": "6.24.1",
        "babel-preset-react": "6.24.1",
        "babel-preset-react-hmre": "1.1.1",
        "babel-preset-stage-1": "6.24.1",
        "cross-env": "^5.0.5",
        "eslint": "4.3.0",
        "eslint-config-airbnb": "15.1.0",
        "eslint-plugin-import": "2.7.0",
        "eslint-plugin-jsx-a11y": "5.1.1",
        "eslint-plugin-react": "7.1.0",
        "husky": "0.14.3",
        "lint-staged": "4.0.2",
        "rimraf": "^2.6.2"
